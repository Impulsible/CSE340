<nav id="site-nav" class="site-nav" aria-label="Primary">
  <ul class="nav-list">

    <% 
      // Define links and their active state
      const links = [
        { href: '/', label: 'Home', active: title && title.startsWith('CSE Motors') },
        { href: '/custom', label: 'Custom', active: title && title.startsWith('Custom') },
        { href: '/sedan', label: 'Sedan', active: title && title.startsWith('Sedan') },
        { href: '/suv', label: 'SUV', active: title && title.startsWith('SUV') },
        { href: '/truck', label: 'Truck', active: title && title.startsWith('Truck') },
        { href: '/account', label: 'Account', active: title && title.startsWith('My Account') }
      ];
    %>

    <% links.forEach(link => { %>
      <li>
        <a href="<%= link.href %>" 
           <%= link.active ? 'class="current-page" aria-current="page"' : '' %>>
          <%= link.label %>
        </a>
      </li>
    <% }) %>

  </ul>
</nav>
